{
  "name": "auto-browser",
  "description": "Light abstraction onto puppeteer in order to simplify easy browser automation task",
  "version": "1.0.3",
  "author": "Tim Bernhard @GenieTim",
  "bin": {
    "auto-browser": "./bin/run.js"
  },
  "bugs": "https://github.com/GenieTim/auto-browser/issues",
  "dependencies": {
    "@inquirer/prompts": "^8.3.0",
    "@oclif/core": "^4.8.1",
    "@oclif/plugin-help": "^6.2.37",
    "@oclif/plugin-version": "^2.2.36",
    "glob": "^13.0.6",
    "puppeteer": "^24.37.5"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "eslint": "^10.0.2",
    "eslint-config-oclif": "^6.0.144",
    "eslint-plugin-unicorn": "^63.0.0",
    "globby": "^16.1.1",
    "prettier": "^3.8.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "files": [
    "/bin",
    ".oclif.manifest.json",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/GenieTim/auto-browser",
  "keywords": [
    "automation",
    "browser",
    "puppeteer"
  ],
  "license": "GPL-3.0-or-later",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "auto-browser",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ]
  },
  "repository": "GenieTim/auto-browser",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
